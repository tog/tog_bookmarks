<div class="titleB">Edit bookmark</div>
<br />

<% unless @bookmark.errors.empty? -%>
<div class="error">
  <%= error_messages_for :bookmark %>
</div>
<% end -%>

<% form_for @bookmark, :url => member_bookmarks_bookmark_path(@bookmark), :html => { :method => :put} do |f| -%>

    <%= render :partial => 'form', :locals => {:f => f}%>
    <div class="actions">
      <%= submit_tag I18n.t('tog_bookmarks.member.save'), :class => "button" %>
    </div>
  </fieldset>
<% end %>